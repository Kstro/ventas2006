<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Region" table="region">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="reNombre" type="string" column="re_nombre" length="50" nullable="false"/>
    <field name="reUsuarioCreacion" type="string" column="re_usuario_creacion" length="15" nullable="false"/>
    <field name="reUsuarioModificacion" type="string" column="re_usuario_modificacion" length="15" nullable="true"/>
    <field name="reFechaCreacion" type="datetime" column="re_fecha_creacion" nullable="false"/>
    <field name="reFechaModificacion" type="datetime" column="re_fecha_modificacion" nullable="true"/>
  </entity>
</doctrine-mapping>
