	<queryString>
		<![CDATA[select so.id_mnt_expediente, er.er_edad_paciente, di.di_resultado, di.di_conclusion, em.nom, es.es_nombre, so_region, pa.nom_pa, so.id_mnt_aten_area_mod_estab
from diagnostico di
join estudio_radiologico er on di.id_estudio_radiologico = er.id
join solicitud so on er.id_solicitud = so.id
join estudio_area ea on so.id_estudio_area = ea.id
join estudio es on ea.id_estudio = es.id
join dblink('dbname=siap port=5432 host=localhost user=hnr_sircim password=Sircim2013*', 'select "Nombre", "Idempleado" from vw_empleado_info') 
as em(nom text, id text) on di.idempleado=em.id
join dblink('dbname=siap port=5432 host=localhost user=hnr_sircim password=Sircim2013*', 'select "Nombre", "NumExpediente" from vw_paciente_info')
as pa(nom_pa text, id_pa text) on so.id_mnt_expediente=pa.id_pa
where so.id_mnt_expediente =$P{registro} and di.id_estudio_radiologico=$P{estudio_radiologico};]]>
	</queryString>
