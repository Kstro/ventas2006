	<queryString>
		<![CDATA[select
t.exp "Registro",
t.nom "Nombre",
t.ed "Edad",
e.es_nombre "Estudio",
s.so_region "Region",
case id_tamano when 1 then ertp.ertp_aceptadas end as "8x10",
case id_tamano when 2 then ertp.ertp_aceptadas end as "10x12",
case id_tamano when 3 then ertp.ertp_aceptadas end as "11x14",
case id_tamano when 4 then ertp.ertp_aceptadas end as "14x14",
case id_tamano when 5 then ertp.ertp_aceptadas end as "14x17",
id_mnt_aten_area_mod_estab "Servicio",
w.nom "Tecnico"
from
solicitud s join estudio_area ea on (ea.id=s.id_estudio_area)
join estudio e on (ea.id_estudio=e.id) join estudio_radiologico er on (s.id=er.id_solicitud)
join estudio_rad_tam_placa ertp on (er.id=ertp.id_estudio_radiologico)
join tamano_placa tp on (ertp.id_tamano=tp.id)
join dblink('dbname=siap port=5432
host=localhost user=hnr_sircim password=Sircim2013*',
'select "CodEmpleado", "Nombre" from vw_empleado_info')
as w(cod character varying(7), nom character varying(100))
on (er.idempleado=w.cod)
join dblink('dbname=siap port=5432
host=localhost user=hnr_sircim password=Sircim2013*',
'select "NumExpediente", "Nombre", "Edad" from vw_paciente_info')
as t(exp character varying(10), nom text,ed text)
on (s.id_mnt_expediente=t.exp);]]>
	</queryString>
