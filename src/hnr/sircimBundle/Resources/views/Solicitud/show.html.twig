{% extends 'hnrsircimBundle::Layout.html.twig' %}

{% block body -%}
    <h1>Solicitud</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Sotipo</th>
                <td>{{ entity.soTipo }}</td>
            </tr>
            <tr>
                <th>Sofecha</th>
                <td>{{ entity.soFecha|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Soregion</th>
                <td>{{ entity.soRegion }}</td>
            </tr>
            <tr>
                <th>Soposicion</th>
                <td>{{ entity.soPosicion }}</td>
            </tr>
            <tr>
                <th>Idmntexpediente</th>
                <td>{{ entity.idMntExpediente }}</td>
            </tr>
            <tr>
                <th>Idmntatenareamodestab</th>
                <td>{{ entity.idMntAtenAreaModEstab }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('solicitud') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('solicitud_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('solicitud_delete', { 'id': entity.id }) }}" method="post">
            <input type="hidden" name="_method" value="DELETE" />
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}
