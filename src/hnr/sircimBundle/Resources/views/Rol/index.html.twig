{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Rol list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Ronombre</th>
                <th>Rodescripcion</th>
                <th>Romodificable</th>
                <th>Roestado</th>
                <th>Rousuariocreacion</th>
                <th>Rousuariomodificacion</th>
                <th>Rofechacreacion</th>
                <th>Rofechamodificacion</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('rol_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.roNombre }}</td>
                <td>{{ entity.roDescripcion }}</td>
                <td>{{ entity.roModificable }}</td>
                <td>{{ entity.roEstado }}</td>
                <td>{{ entity.roUsuarioCreacion }}</td>
                <td>{{ entity.roUsuarioModificacion }}</td>
                <td>{% if entity.roFechaCreacion %}{{ entity.roFechaCreacion|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if entity.roFechaModificacion %}{{ entity.roFechaModificacion|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('rol_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('rol_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('rol_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
