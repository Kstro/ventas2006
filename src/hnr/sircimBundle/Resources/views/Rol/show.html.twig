{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Rol</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Ronombre</th>
                <td>{{ entity.roNombre }}</td>
            </tr>
            <tr>
                <th>Rodescripcion</th>
                <td>{{ entity.roDescripcion }}</td>
            </tr>
            <tr>
                <th>Romodificable</th>
                <td>{{ entity.roModificable }}</td>
            </tr>
            <tr>
                <th>Roestado</th>
                <td>{{ entity.roEstado }}</td>
            </tr>
            <tr>
                <th>Rousuariocreacion</th>
                <td>{{ entity.roUsuarioCreacion }}</td>
            </tr>
            <tr>
                <th>Rousuariomodificacion</th>
                <td>{{ entity.roUsuarioModificacion }}</td>
            </tr>
            <tr>
                <th>Rofechacreacion</th>
                <td>{{ entity.roFechaCreacion|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Rofechamodificacion</th>
                <td>{{ entity.roFechaModificacion|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('rol') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('rol_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('rol_delete', { 'id': entity.id }) }}" method="post">
            <input type="hidden" name="_method" value="DELETE" />
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}
