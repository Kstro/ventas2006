{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Region list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Renombre</th>
                <th>Reusuariocreacion</th>
                <th>Reusuariomodificacion</th>
                <th>Refechacreacion</th>
                <th>Refechamodificacion</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('region_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.reNombre }}</td>
                <td>{{ entity.reUsuarioCreacion }}</td>
                <td>{{ entity.reUsuarioModificacion }}</td>
                <td>{% if entity.reFechaCreacion %}{{ entity.reFechaCreacion|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if entity.reFechaModificacion %}{{ entity.reFechaModificacion|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('region_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('region_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('region_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
